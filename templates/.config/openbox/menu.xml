    <item label="LibreOffice Math"><action name="Execute"><command>libreoffice --math></command></action></item>
    <item label="LibreOffice Writer"><action name="Execute"><command>libreoffice --writer></command></action></item>
  </menu>
  <menu id="tools" label="Tools">
	  <menu id="web" label="Web Apps">
		<item label="Burpsuite"><action name="Execute"><command>burpsuite</command></action></item>
