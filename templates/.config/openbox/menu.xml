<?xml version="1.0" encoding="utf-8"?>
<openbox_menu xmlns="http://openbox.org/"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://openbox.org/">
  <menu id="root-menu" label="Applications">
    <item label="File Manager"><action name="Execute"><command>nautilus</command></action></item>
    <item label="Web Browser"><action name="Execute"><command>firefox</command></action></item>
  <separator/>
  <menu id="text-editor" label="Text Editor">
    <item label="Android Studio"><action name="Execute"><command>android-studio></command></action></item>
    <item label="Arduino IDE"><action name="Execute"><command>arduino></command></action></item>
    <item label="Geany"><action name="Execute"><command>geany</command></action></item>
 </menu>
  <menu id="graphics" label="Graphics">
